<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="QualityNutritionPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Modelo.DAntropometricos</class>
    <class>Modelo.Alimentos</class>
    <class>Modelo.Pacientes</class>
    <class>Modelo.ComposicionCorporal</class>
    <class>Modelo.Cita</class>
    <class>Modelo.Dieta</class>
    <class>Modelo.DAnaliticos</class>
    <class>Modelo.Usuarios</class>
    <class>Control.DAntropometricos</class>
    <class>Control.DAnaliticos</class>
    <class>Control.Pacientes</class>
    <class>Control.Dieta</class>
    <class>Control.ComposicionCorporal</class>
    <class>Control.Alimentos</class>
    <class>Control.Cita</class>
    <class>Control.Usuarios</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/quality nutrition?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
